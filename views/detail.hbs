<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css"
    />
    <link
      rel="shortcut icon"
      href="/assets/image/dumbways.png"
      type="image/x-icon"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH"
      crossorigin="anonymous"
    />
    <title>My website</title>
  </head>
  <body>
    <nav
      class="navbar navbar-expand-lg bg-dark-subtle text-dark-emphasis p-3 m-1"
    >
      <div class="container-fluid">
        <img src="/assets/image/dumbways.png" alt="" width="50" />
        <a class="navbar-brand" href="/"></a>
      </div>
    </nav>
    <div class="container shadow p-3 mb-5 bg-secondary-subtle rounded">
      <div
        class="card mb-3 border border-0"
        style="max-width: 100%; height: max-content"
      >
        <div class="row g-0">
          <div
            class="col-md-4 border border-0 rounded shadow bg-body-tertiary rounded"
            style="max-width: 100%; height: max-content"
          >
            <img
              src="{{project.image}}"
              class="img-fluid rounded-start"
            />
          </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title">{{project.title}}</h5>
              <p>Start Date : {{project.start}}</p>
              <p>End Date : {{project.end}}</p>
              <p>Durasi : {{project.duration}}</p>
              <p class="card-text">
                {{project.desk}}
              </p>
            </div>
          </div>
        </div>
      </div>
       <a class="btn btn-dark col-1 m-2" href="/" role="button" >Kembali</a>
    </div>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
      crossorigin="anonymous"
    ></script>
  </body>
</html>